---
import favIcon from '~/assets/favicons/axionlogo.png';
import favIconSvg from '~/assets/favicons/axionlogo.svg';
import appleTouchIcon from '~/assets/favicons/axionlogo.png';
---

<link rel="shortcut icon" href={favIcon.src} />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
